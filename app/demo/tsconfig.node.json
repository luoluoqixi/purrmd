{
  "include": ["vite.config.ts"],
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "composite": true,
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "resolveJsonModule": true,
    "sourceMap": false,
    "strict": true,
    "skipLibCheck": true,
    "types": ["@types/node"]
  }
}
